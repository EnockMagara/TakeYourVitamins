            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">// Save token and user id to JMeter properties so they can be used by all thread groups
int userNum = ctx.getThreadNum();
props.put("token_" + userNum, vars.get("testUserToken"));
props.put("userId_" + userNum, vars.get("testUserId"));</stringProp>
          </BeanShellPostProcessor>
          <hashTree/>
        </hashTree>
