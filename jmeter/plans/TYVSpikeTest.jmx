          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">// Get user token and ID based on thread number
int userNum = ctx.getThreadNum() % 5; // We created 5 users in setup
String token = props.get("token_" + userNum);
String userId = props.get("userId_" + userNum);

if (token != null) {
    vars.put("userToken", token);
    vars.put("userId", userId);
}</stringProp>
        </BeanShellPreProcessor>
